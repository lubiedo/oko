{
  "manifest_version": 2,
  "name": "oko",
  "description": "Remove UTM query parameters from HTTP requests",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "oko (enabled)"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ]
}
